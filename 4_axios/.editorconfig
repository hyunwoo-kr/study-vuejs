root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

[*.{html,css,js}]
indent_size = 2
indent_style = tab

[*.vue]
indent_size = 2
indent_style = tab

[*.py]
indent_style = space
indent_size = 4
insert_final_newline = true

[*.{yml, yaml}]
indent_style = tab
indent_size = 4
